{
    "name": "arnulfosolis/qcharts",
    "homepage": "https://github.com/arnulfojr/qcharts",
    "description": "Business tool desing to aid the analysis of SQL data in a graphical way to empower users among a team.",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "version": "1.0",
    "keywords": ["query", "formatter", "charter", "qcharts", "sql", "grapher", "graph", "chart"],
    "authors": [{
      "name": "Arnulfo Solis Ramirez",
      "email": "arnulfojr@kuzzy.com",
      "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "QCharts\\": "QCharts/"
        }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "2.7.*",
        "doctrine/orm": "^2.4.8",
        "symfony/assetic-bundle": "~2.3",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "jdorn/sql-formatter": "^1.2",
        "mtdowling/cron-expression": "1.1.*",
        "nelmio/api-doc-bundle": "2.*"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "phpunit/phpunit" : "4.*",
        "phpspec/phpspec":"2.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
